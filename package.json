{
  "name": "kentcdodds.github.io",
  "version": "1.0.0",
  "private": true,
  "description": "my personal website",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged && npm run validate",
    "build:clean": "rimraf dist",
    "build:copy": "npm-run-all --parallel build:copy:*",
    "build:copy:copyDir": "cpy \"**/*.*\" ../dist/ --parents --cwd=./copy",
    "build:copy:posts": "cpy \"**/*.!(js)\" ../../../../dist/post --parents --cwd=./src/pages/blog/posts",
    "build:pages": "babel-node generate/",
    "build": "npm-run-all --parallel build:*",
    "test": "npm run lint",
    "lint": "eslint .",
    "format": "prettier-eslint --write \"src/**/*.js\" \"generate/**/*.js\"",
    "serve": "http-server dist",
    "validate": "npm-run-all --parallel lint build",
    "storybook": "cross-env STORYBOOK_RUNNING=true start-storybook --port 9001 --dont-track"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentcdodds/kentcdodds.github.io.git"
  },
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentcdodds/kentcdodds.github.io/issues"
  },
  "homepage": "https://github.com/kentcdodds/kentcdodds.github.io#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.0",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "cpy-cli": "^1.0.1",
    "cross-env": "^3.1.4",
    "cross-spawn": "^5.0.1",
    "eslint": "^3.16.1",
    "eslint-config-kentcdodds": "^11.1.0",
    "eslint-plugin-jsx": "^0.0.2",
    "eslint-plugin-react": "^6.10.0",
    "glamor": "^2.20.24",
    "glob": "^7.1.1",
    "http-server": "^0.9.0",
    "husky": "^0.13.1",
    "lint-staged": "^3.3.1",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "moment": "^2.17.1",
    "npm-run-all": "^4.0.2",
    "prettier-eslint-cli": "^3.0.1",
    "random-emoji": "^1.0.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.0",
    "slugify": "^1.1.0",
    "strip-indent": "^2.0.0",
    "uglify-js": "^2.7.5"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/react",
      "kentcdodds/babel-react-require"
    ],
    "rules": {
      "react/jsx-max-props-per-line": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
