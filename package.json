{
  "name": "kentcdodds.github.io",
  "version": "1.0.0",
  "private": true,
  "description": "My Personal Website",
  "main": "index.js",
  "scripts": {
    "build:clean": "rimraf dist",
    "build:copy": "npm-run-all --parallel build:copy:*",
    "build:copy:copyDir": "cpy \"**/*.*\" ../dist/ --parents --cwd=./copy",
    "build:copy:posts": "cpy \"**/*.!(js)\" ../../../../dist/post --parents --cwd=./src/pages/blog/posts",
    "build:pages": "babel-node generate/",
    "build": "npm-run-all --parallel build:*",
    "test": "npm run lint",
    "lint": "kcd-scripts lint",
    "serve": "http-server dist",
    "validate": "kcd-scripts validate",
    "precommit": "kcd-scripts precommit",
    "storybook": "cross-env STORYBOOK_RUNNING=true start-storybook --port 9001"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/kentcdodds.com.git"
  },
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kentcdodds/kentcdodds.com/issues"
  },
  "homepage": "https://github.com/kentcdodds/kentcdodds.com#readme",
  "devDependencies": {
    "@storybook/react": "^3.2.12",
    "babel-cli": "^6.24.0",
    "babel-macros": "^1.1.0",
    "babel-plugin-react-require": "^3.0.0",
    "common-tags": "^1.4.0",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.0.5",
    "cross-spawn": "^5.1.0",
    "eslint-config-kentcdodds": "^13.0.1",
    "glamor": "^2.20.40",
    "glamorous": "^4.8.0",
    "glob": "^7.1.1",
    "http-server": "^0.10.0",
    "import-all.macro": "^2.0.0",
    "kcd-scripts": "^0.27.0",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "moment": "^2.19.1",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.6.0",
    "random-emoji": "^1.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.2.0",
    "slugify": "^1.1.0",
    "strip-indent": "^2.0.0",
    "uglify-js": "^3.1.3"
  }
}
