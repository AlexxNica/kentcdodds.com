{
  "private": true,
  "name": "kentcdodds-com",
  "homepage": "https://kentcdodds.com",
  "twitter": "kentcdodds",
  "repository": "https://github.com/kentcdodds/kentcdodds.github.io.git",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix .",
    "start": "phenomic start",
    "build": "phenomic build",
    "pretest": "npm run lint",
    "test": "npm run build"
  },
  "phenomic": {
    "CNAME": true,
    "nojekyll": true,
    "offline": true,
    "open": false
  },
  "babel": {
    "presets": [
      "babel-preset-react",
      "babel-preset-es2015",
      "babel-preset-stage-1"
    ],
    "plugins": [
      "react-require"
    ],
    "env": {
      "production": {
        "presets": [
          "babel-preset-react-optimize"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds/react",
      "kentcdodds/import",
      "kentcdodds/webpack"
    ],
    "rules": {
      "react/prefer-stateless-function": "error",
      "react/react-in-jsx-scope": "off"
    },
    "settings": {
      "import/extensions": [
        "js"
      ]
    }
  },
  "eslintConfigRuleReact/prefer-stateless-function": "https://github.com/MoOx/phenomic/issues/46",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^6.0.0-beta.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-require": "2.1.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "css-loader": "^0.23.0",
    "eslint": "^2.0.0",
    "eslint-config-kentcdodds": "6.2.1",
    "eslint-import-resolver-webpack": "0.3.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.1",
    "history": "^2.0.0",
    "invariant": "^2.1.1",
    "json-loader": "^0.5.2",
    "npm-run-all": "^1.7.0",
    "phenomic": "^0.14.2",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-loader": "^0.7.0",
    "postcss-reporter": "^1.3.0",
    "raw-loader": "^0.5.1",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-helmet": "^3.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.3.0",
    "react-svg-inline": "^1.1.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "redux": "^3.0.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.1",
    "whatwg-fetch": "^0.11.0"
  }
}
